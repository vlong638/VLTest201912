<Views>
	<!--IsNeedOnPage="false" DisplayName="" (.+) Description="(\w+)"(.+)\r\n
	IsNeedOnPage="true" DisplayName="$2" $1 Description="$2"$3\r\n-->
	<View ViewName="User" ViewURL="../../api/System/GetCommonSelectByDirectoryName">
		<Wheres>
			<!--"DisplayType <类型[1-文本框|2-数字框|3-单选下拉框|4-树状下拉框|5-日期选择框][默认:1]>",-->
			<Where ComponentName="name" DisplayName="角色名称" DisplayType="1"/>
		</Wheres>
		<OrderBy DefaultName ="id" DefaultValue ="desc"/>
		<Property IsNeedOnPage="true" DisplayName="标识号" ColumnName="id" DisplayType="None" DisplayWidth="100" IsSortable="False"/> <!--DataType="bigint" Description="-"-->
		<Property IsNeedOnPage="true" DisplayName="角色名称" ColumnName="name" DisplayType="None" DisplayWidth="100" IsSortable="False"/> <!--DataType="nvarchar" Description="-"-->
	</View>
</Views>