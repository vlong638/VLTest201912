			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="纯母乳喂养人数" ColumnName="murucwy" DisplayWidth="80"/>
			
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="新生儿访视人数" ColumnName="xinshengfs" DisplayWidth="80"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="7岁以下儿童保健人数" ColumnName="qisuibjs" DisplayWidth="80"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="3岁以下儿童健康管理人数" ColumnName="jkguanlis" DisplayWidth="80"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="3岁以下儿童系统管理人数" ColumnName="guanlis" DisplayWidth="80"/>
			
