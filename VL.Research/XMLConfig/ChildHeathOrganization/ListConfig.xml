			<Property DisplayLevel="3" IsNeedOnPage="true" DisplayName="女" ColumnName="yinersrnv" DisplayWidth="60"/>
			<Property DisplayLevel="3" IsNeedOnPage="true" DisplayName="性别不明" ColumnName="yinersbm" DisplayWidth="60"/>
			<!--新生儿死亡数-->
			<Property DisplayLevel="3" IsNeedOnPage="true" DisplayName="合计" ColumnName="babys" DisplayWidth="60"/>
			<Property DisplayLevel="3" IsNeedOnPage="true" DisplayName="男" ColumnName="babynans" DisplayWidth="60"/>
			<Property DisplayLevel="3" IsNeedOnPage="true" DisplayName="女" ColumnName="babynvs" DisplayWidth="60"/>
			<Property DisplayLevel="3" IsNeedOnPage="true" DisplayName="性别不明" ColumnName="babybms" DisplayWidth="60"/>
			<!--身高(长)体重检查-->
			<Property DisplayLevel="3" IsNeedOnPage="true" DisplayName="检查人数" ColumnName="tzjcrs" DisplayWidth="80"/>
