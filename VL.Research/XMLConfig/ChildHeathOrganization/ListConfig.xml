<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Views>
	<!--IsNeedOnPage="false" DisplayName="" (.+) Description="(\w+)"(.+)\r\n
	IsNeedOnPage="true" DisplayName="$2" $1 Description="$2"$3\r\n-->
	<View ViewName="GWE" ViewURL="../api/System/GetCommonSelectByDirectoryName">
		<Export URL="../api/System/CommonExportAll"/>
		<Wheres>
			<Where ComponentName="日期范围" Value="" DisplayName="日期范围" DisplayType="5" Required="true"/>
			<Where ComponentName="单位名称" Value="" DisplayName="单位名称" DisplayType="1"/>
			<!--"DisplayType <类型[1-文本框|2-数字框|3-单选下拉框|4-树状下拉框|5-日期选择框][默认:1]>",-->
			<!--<Where ComponentName="序号" Value="" DisplayName="序号" DisplayType="1"/>-->
		</Wheres>
		<OrderBy DefaultName ="createdate" DefaultValue ="desc"/>
		<Properties>
			<Property DisplayLevel="1" RowSpan="3" IsNeedOnPage="true" DisplayName="填报单位" ColumnName="fuwujgmc" DisplayWidth="180" IsSortable="False"/>
			<Property DisplayLevel="1" RowSpan="1" ColumnSpan="4" IsNeedOnPage="true" DisplayName="活产数"/>
			<Property DisplayLevel="1" RowSpan="1" ColumnSpan="4" IsNeedOnPage="true" DisplayName="儿童数"/>
			<Property DisplayLevel="1" RowSpan="1" ColumnSpan="12" IsNeedOnPage="true" DisplayName="5岁以下儿童死亡情况"/>
			<Property DisplayLevel="1" RowSpan="1" ColumnSpan="3" IsNeedOnPage="true" DisplayName="6个月内婴儿母乳喂养情况"/>
			<Property DisplayLevel="1" RowSpan="1" ColumnSpan="3" IsNeedOnPage="true" DisplayName="7岁以下儿童保健服务"/>
			<Property DisplayLevel="1" RowSpan="1" ColumnSpan="8" IsNeedOnPage="true" DisplayName="5岁以下儿童营养评价"/>
			<Property DisplayLevel="1" RowSpan="1" ColumnSpan="3" IsNeedOnPage="true" DisplayName="0-6岁儿童眼保健和视力检查"/>
			<!--活产数-->
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="合计" ColumnName="huochanzs" DisplayWidth="60"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="男" ColumnName="huochannan" DisplayWidth="60"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="女" ColumnName="huochannv" DisplayWidth="60"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="性别不明" ColumnName="huochanbm" DisplayWidth="60"/>
			<!--儿童数-->
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="7岁以下" ColumnName="child7" DisplayWidth="60"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="5岁以下" ColumnName="child5" DisplayWidth="60"/>
			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="3岁以下" ColumnName="child3" DisplayWidth="60"/>

			<Property DisplayLevel="2" RowSpan="1" ColumnSpan="4" IsNeedOnPage="true" DisplayName="5岁以下儿童死亡数"/>
			<Property DisplayLevel="2" RowSpan="1" ColumnSpan="4" IsNeedOnPage="true" DisplayName="婴儿死亡数"/>
			<Property DisplayLevel="2" RowSpan="1" ColumnSpan="4" IsNeedOnPage="true" DisplayName="新生儿死亡数"/>

			<Property DisplayLevel="2" RowSpan="2" ColumnSpan="1" IsNeedOnPage="true" DisplayName="调查人数" ColumnName="muruzs" DisplayWidth="80"/>
